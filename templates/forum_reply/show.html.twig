{% extends 'base.html.twig' %}

{% block title %}ForumReply{% endblock %}

{% block body %}
    <h1>ForumReply</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ forum_reply.id }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ forum_reply.content }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
